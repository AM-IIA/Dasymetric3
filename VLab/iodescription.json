{
    "inputs": [   
       {
        "id": "census_pop_vector_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Population count per census zone",
        "name": "POP vector input",
        "obligation": true,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "POP.zip"
     },
	   {
        "id": "pop_field",
        "inputType": "individual",
        "valueType": "value",
        "description": "Name of population field of attributes",
		"name": "POP_field",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "string_parameter",
		"defaultValue":"POP"
	 },
       {
        "id": "urban_atlas_vector_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Urban Atlas from Copernicus Land Monitoring Service",
        "name": "Urban Atlas",
        "obligation": true,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "UrbanAtlas.zip"
     },
	    {
        "id": "buildings_raster_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Binary Built-up map",
        "name": "Buildings",
        "obligation": true,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "Buildings.zip"
     },	 
        {
        "id": "heights_raster_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Building heights",
		"name": "Building heights (optional)",
        "obligation": false,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "Heights.zip"
     },
	    {
        "id": "weights",
        "inputType": "array",
        "valueType": "keyValue",
        "description": "Correction factors per any specfic building use. Insert this sequence: Residential",
		"name": "Weights",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "number_parameter",
		"defaultValueArray":"Residential:1.0 Rural:0.7 Ind_Comm:0.1 Roads:0.0 Others:0.01"
      }
       ],
    "outputs": [
        {
        "id": "pop_grid_vector_layer",
        "outputType": "individual",
        "valueType": "value",
        "description": "Grid population map (100 m x 100 m)",
		"name": "POP_GRID",
		"hasDefault": true,
		"defaultValue":"POP_GRID.gpkg",
		"valueSchema": "url",
        "target": "POP_GRID.gpkg"
     },	
        {
        "id": "log",
        "outputType": "individual",
        "valueType": "value",
        "description": "Log file",
		"name": "log",
		"valueSchema": "url",
        "target": "log.txt"
     }		 
      ]
}