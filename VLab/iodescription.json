{
    "inputs": [   
       {
        "id": "census_pop_vector_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Population count per census zone",
        "name": "POP vector input",
        "obligation": true,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "POP.zip"
     },
	   {
        "id": "pop_field",
        "inputType": "individual",
        "valueType": "value",
        "description": "Name of population attribute field",
		"name": "POP_field",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "string_parameter",
		"defaultValue":"POP"
	 },
       {
        "id": "urban_atlas_vector_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Urban Atlas from Copernicus Land Monitoring Service.  A generic urban environment classification dataset can be used instead of urban Atlas for not-European region. See customizable parameters",
        "name": "Urban Atlas",
        "obligation": true,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "UrbanAtlas.zip"
     },
	   {
        "id": "class_encoding_field",
        "inputType": "individual",
        "valueType": "value",
        "description": "Name of the field with the class encoding"
		"name": "POP_field",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "string_parameter",
		"defaultValue":"POP"
	 },
	    {
        "id": "classes",
        "inputType": "array",
        "valueType": "value",
        "description": "Weights",
		"name": "weights",
        "obligation": false,
		"valueArrayInterpretation":"replace"
      },
   	    {
        "id": "buildings_raster_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Binary Built-up map",
        "name": "Buildings",
        "obligation": true,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "Buildings.zip"
     },	 
        {
        "id": "heights_raster_layer",
        "inputType": "individual",
        "valueType": "value",
        "description": "Building heights",
		"name": "Building heights (optional)",
        "obligation": false,
        "hasDefault": false,
        "valueSchema": "url",
        "target": "Heights.zip"
     },
	    {
        "id": "Residential_weight",
        "inputType": "individual",
        "valueType": "value",
        "description": "Weight for residential building use areas",
		"name": "residential_weight",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "number_parameter",
		"defaultValue":1.0
      },
	    {
        "id": "Rural_weight",
        "inputType": "individual",
        "valueType": "value",
        "description": "Weight for rural building use areas",
		"name": "rural_weight",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "number_parameter",
		"defaultValue":0.7
      },
	    {
        "id": "IndComm_weight",
        "inputType": "individual",
        "valueType": "value",
        "description": "Weight for industrial, commercial and leisure building use areas",
		"name": "ind_comm_weight",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "number_parameter",
		"defaultValue":0.1
      },
	    {	  
        "id": "Roads_weight",
        "inputType": "individual",
        "valueType": "value",
        "description": "Weight for roads, railways, port and airport building use areas",
		"name": "roads_weight",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "number_parameter",
		"defaultValue":0.0
      },
	    {
        "id": "Others_weight",
        "inputType": "individual",
        "valueType": "value",
        "description": "Weight for other building use classes",
		"name": "others_weight",
        "obligation": false,
        "hasDefault": true,
        "valueSchema": "number_parameter",
		"defaultValue":0.01
      },
	    {
        "id": "weights",
        "inputType": "array",
        "valueType": "value",
        "description": "Weights",
		"name": "weights",
        "obligation": false,
		"valueSchema": "string_parameter",
		"valueArrayInterpretation":"replace"
      }
       ],
    "outputs": [
        {
        "id": "pop_grid_vector_layer",
        "outputType": "individual",
        "valueType": "value",
        "description": "Grid population map (100 m x 100 m)",
		"name": "POP_GRID",
		"hasDefault": true,
		"defaultValue":"POP_GRID.gpkg",
		"valueSchema": "url",
        "target": "POP_GRID.gpkg"
     },	
        {
        "id": "log",
        "outputType": "individual",
        "valueType": "value",
        "description": "Log file",
		"name": "log",
		"valueSchema": "url",
        "target": "log.txt"
     }		 
      ]
}